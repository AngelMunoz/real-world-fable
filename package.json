{
  "name": "real-world-fable",
  "version": "1.0.0",
  "description": "> ### Fable and F# codebase containing real world examples (CRUD, auth, advanced patterns, etc) that adheres to the [RealWorld](https://github.com/gothinkster/realworld) spec and API.",
  "dependencies": {
    "markdown": "^0.5.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "mocha": "^6.2.2",
    "fuse-box": "4.0.0"
  },
  "scripts": {
    "prestart": "dotnet tool restore",
    "start": "dotnet fable watch ./src --run node ./fusebox.js watch",
    "prebuild": "dotnet tool restore",
    "build": "dotnet fable ./src && node ./fusebox.js prod",
    "test:live": "dotnet fable watch tests --run node ./fusebox.js test watch",
    "build:test": "dotnet fable tests && node ./fusebox.js test",
    "pretest": "dotnet tool restore",
    "test": "dotnet fable tests --outDir dist/tests && mocha dist/tests -r esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thitemple/realworld-fsharp-fable.git"
  },
  "keywords": [
    "F#",
    "fsharp",
    "fable",
    "elmish",
    "react",
    "real world"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thitemple/realworld-fsharp-fable/issues"
  },
  "homepage": "https://github.com/thitemple/realworld-fsharp-fable#readme"
}
